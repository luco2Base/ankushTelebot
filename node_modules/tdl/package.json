{
  "name": "tdl",
  "version": "8.0.1",
  "description": "Node.js bindings to TDLib (Telegram Database library)",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build:lib": "tsc",
    "build:addon": "node-gyp rebuild",
    "build": "tsc && node scripts/copy-readme.js && node scripts/generate-flow.js && node-gyp rebuild",
    "make-prebuild": "prebuildify --napi -t 18.20.2",
    "clean": "node-gyp clean",
    "install": "node-gyp-build"
  },
  "files": [
    "index.d.ts",
    "dist",
    "addon",
    "binding.gyp",
    "prebuilds",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "debug": "^4.3.4",
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.1"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "author": "Bannerets <comonoid@protonmail.com> (https://github.com/Bannerets)",
  "license": "MIT",
  "keywords": [
    "telegram",
    "telegram-api",
    "telegram-client-api",
    "tdlib",
    "tglib",
    "bindings",
    "node-addon",
    "backend"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bannerets/tdl.git"
  },
  "bugs": {
    "url": "https://github.com/Bannerets/tdl/issues"
  },
  "homepage": "https://github.com/Bannerets/tdl#readme"
}
